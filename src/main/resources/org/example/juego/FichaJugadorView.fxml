<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Arc?>

<AnchorPane fx:id="rootPane"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.juego.controlador.FichaJugadorController"
            prefHeight="350.0" prefWidth="600.0">
    <children>
        <Group fx:id="group">
            <children>
                <Arc fx:id="arc1" startAngle="0" length="60" type="ROUND"   fill="#ffffff" stroke="red" strokeWidth="4"/>
                <Arc fx:id="arc2" startAngle="60" length="60" type="ROUND"  fill="#ffffff" stroke="orange" strokeWidth="4"/>
                <Arc fx:id="arc3" startAngle="120" length="60" type="ROUND" fill="#ffffff" stroke="blue" strokeWidth="4"/>
                <Arc fx:id="arc4" startAngle="180" length="60" type="ROUND" fill="#ffffff" stroke="green" strokeWidth="4"/>
                <Arc fx:id="arc5" startAngle="240" length="60" type="ROUND" fill="#ffffff" stroke="yellow" strokeWidth="4"/>
                <Arc fx:id="arc6" startAngle="300" length="60" type="ROUND" fill="#ffffff" stroke="purple" strokeWidth="4"/>
            </children>
        </Group>
        <!-- <Button text="Resaltar Aleatorio" onAction="#onResaltarAleatorio" layoutX="230.0" layoutY="300.0" /> -->
    </children>
</AnchorPane>